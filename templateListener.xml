<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/templateListener.xsd">
    <import>
        <templatelistener name="threadNext">
            <environment>user</environment>
            <templatename>thread</templatename>
            <eventname>contentHeaderNavigation</eventname>
            <templatecode><![CDATA[{include file='__nextThreadButton' application='wbb'}]]></templatecode>
        </templatelistener>

        <templatelistener name="threadNextJavascript">
            <environment>user</environment>
            <templatename>headIncludeJavaScript</templatename>
            <eventname>javascriptInclude</eventname>
            <templatecode><![CDATA[{include file='__nextThreadJavascript' application='wbb'}]]></templatecode>
        </templatelistener>
    </import>
</data>
